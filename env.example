# Calry OTA API configuration (server-side)
# Do NOT expose the API key to the client. Only server components should read these.
CALRY_API_URL=
CALRY_API_KEY=
CALRY_TENANT_ID=
# Optional display currency e.g., USD, EUR, AED
CALRY_DISPLAY_CURRENCY=
# Public tenant site base URL for property links (e.g., https://tenant.example.com)
TENANT_PUBLIC_URL=
# Database
# DATABASE_URL="postgresql://user:password@localhost:5432/dbname"

# Authentication
# NEXTAUTH_SECRET="your-secret-key"
# NEXTAUTH_URL="http://localhost:3000"

# Email Service
# EMAIL_SERVER_HOST="smtp.example.com"
# EMAIL_SERVER_PORT=587
# EMAIL_SERVER_USER="your-email@example.com"
# EMAIL_SERVER_PASSWORD="your-password"

# Map Provider (Mapbox/Google Maps)
# MAPBOX_ACCESS_TOKEN="your-mapbox-token"
# GOOGLE_MAPS_API_KEY="your-google-maps-key"

# Payment Processing
# STRIPE_SECRET_KEY="sk_test_..."
# STRIPE_PUBLISHABLE_KEY="pk_test_..."

# Analytics
# GOOGLE_ANALYTICS_ID="G-XXXXXXXXXX"

# Social Media
# FACEBOOK_APP_ID="your-facebook-app-id"
# INSTAGRAM_ACCESS_TOKEN="your-instagram-token"
